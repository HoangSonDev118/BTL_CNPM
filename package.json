{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:auto-migrate && next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "vercel:build": "prisma generate && next build --turbopack",
    "build:local": "npm run db:auto-migrate && next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "db:auto-migrate": "node scripts/auto-migrate.js",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/init-db.js",
    "db:create-admin": "node scripts/create-super-admin.js",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:seed-books": "node scripts/seed-books.js",
    "db:seed-all": "npm run db:seed && npm run db:seed-books",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@prisma/client": "^6.16.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.58.0",
    "antd": "^5.27.4",
    "next": "15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "prisma": "^6.16.3",
    "supabase": "^2.48.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.18 <=22.x"
  }
}
